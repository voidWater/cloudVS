
.panel{
	background-color: #fff;
	border-radius: 20rpx;
	margin: 28rpx 18rpx 18rpx 18rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	padding: 20rpx 30rpx;
	font-size: 36rpx;
}
.list{
	background-color: #fff;
	border-radius: 20rpx;
	margin: 18rpx;
	padding: 18rpx 5rpx;
}
.list .item{
	font-size: 32rpx;
	margin: 20rpx 18rpx;
	padding-bottom: 18rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}

