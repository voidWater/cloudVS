@-webkit-keyframes bounce {
0% {
    top: 50%;
    -webkit-animation-timing-function: ease-in;
            animation-timing-function: ease-in;
}
  /* 动画开始时图片位于原始位置 */
50% {
    top: 40%;
}
  /* 动画进行到一半时，图片向上移动10像素 */
100% {
    top: 50%;
    -webkit-animation-timing-function: ease-out;
            animation-timing-function: ease-out;
}
  /* 动画结束时回到原始位置 */
}
@keyframes bounce {
0% {
    top: 50%;
    -webkit-animation-timing-function: ease-in;
            animation-timing-function: ease-in;
}
  /* 动画开始时图片位于原始位置 */
50% {
    top: 40%;
}
  /* 动画进行到一半时，图片向上移动10像素 */
100% {
    top: 50%;
    -webkit-animation-timing-function: ease-out;
            animation-timing-function: ease-out;
}
